    "concurrently": "^4.1.0",
    "node": "^11.12.0",
    "nodemon": "^1.18.7"
  },
  "engines": {
    "node": "10.3.0",
    "npm": "6.x"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "node": "^11.12.0",
    "react-alert": "^5.2.0",
    "react-bootstrap": "^1.0.0-beta.5"
  }
